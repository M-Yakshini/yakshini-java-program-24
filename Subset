class Solution {
    public List<List<Integer>> subsets(int[] a) {
        List<List<Integer>> res = new ArrayList<>();
        res.add(new ArrayList<>());
        for (int n : a) {
            int s = res.size();
            for (int i = 0; i < s; i++) {
                List<Integer> t = new ArrayList<>(res.get(i));
                t.add(n);
                res.add(t);
            }
        }
        return res;
    }
}
